import random

ne = 0
num = float(input('Digite um numero: '))
escolhido = random.randint(1,10)
while num!=escolhido:
    ne += 1
    if num>escolhido:
        num = float(input("o numero escolhido é menor, tente novamente"))

    else:
        num = float(input("o numero escolhido é maior, tente novamente"))
        

print('Você  tentou {} vezes, ate acertar'.format(ne))

