from datetime import date
anoNascimento = int(input('Digite o ano de seu nascimento: '))
idade = date.today().year - anoNascimento
if idade <= 9:
    print('Categoria mirim, idade {} anos'.format(idade))
elif idade>9 and idade<=14:
    print('Categoria infantil, idade {} anos'.format(idade))
elif idade>14 and idade<=19:
    print('Categoria junior, idade {} anos'.format(idade))
elif idade>19 and idade<=20:
    print('Categoria senior, idade {} anos'.format(idade))
else:
    print('Categoria master, idade {} anos'.format(idade))
