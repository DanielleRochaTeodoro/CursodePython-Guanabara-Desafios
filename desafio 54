from datetime import date
maior = 0
menor = 0
for c in range (1,8):
    data = int(input('Digite seu ano de nascimento: '))
    if date.today().year - data >=18:
        maior = maior +1
    else:
        menor = menor +1
print('{} pessoas ainda não atingiram a maioridade e {} pessoas já atingiram'.format(menor,maior))
